ALERT HighCPUAlert
  IF sum(rate(container_cpu_usage_seconds_total{name=~".+"}[1m]) * 100) > 0
  FOR 1s
  LABELS {severity = "critical"}
  ANNOTATIONS {
    summary = "High CPU usage on 10.0.2.15",
    description = "Development host has been experiencing high cpu usage for more than 1 minutes.",
  }